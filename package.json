{
  "name": "@kyso/jupyterlab",
  "version": "1.10.0",
  "description": "Push to Kyso from JupyterLab",
  "keywords": [
    "kyso",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "MIT",
  "private": false,
  "author": "Eoin Murray",
  "main": "lib/index.js",
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "kyso_jupyterlab"
        }
      }
    }
  },
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.js",
    "lib/**/*.js",
    "schema/*.json",
    "style/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kyso-io/jupyterlab-kyso.git"
  },
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel src -w -d lib"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "@jupyterlab/coreutils": "^2.2.1",
    "@jupyterlab/docmanager": "^0.19.1",
    "@jupyterlab/filebrowser": "^0.19.3",
    "@jupyterlab/mainmenu": "^0.8.1",
    "@jupyterlab/services": "^3.2.1",
    "@kyso/auth-popup": "^1.0.0",
    "@kyso/client": "^4.2.2",
    "@phosphor/algorithm": "^1.1.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "evergreen-ui": "^4.13.0",
    "file-saver": "^2.0.0",
    "js-base64": "^2.5.0",
    "js-cookie": "^2.2.0",
    "jszip": "^3.1.5",
    "rc-progress": "^2.2.5",
    "react": "16.4.2",
    "react-spinkit": "^3.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-watch": "^2.0.6",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3"
  }
}
